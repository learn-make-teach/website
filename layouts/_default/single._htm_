{{ define "main" }}
<div class="container-xxl main-layout mt-0 ms-0 me-0 p-0 ps-5">
    <div class="title ps-3 pt-3">
        <h1>{{ .Title }}</h1>
    </div>
    <main class="my-content p-3">
        {{ .Content }}
    </main>
    <aside class="toc p-3">
        <strong>On this page</strong>
        {{ .TableOfContents }}
    </aside>
</div>
{{ end }}